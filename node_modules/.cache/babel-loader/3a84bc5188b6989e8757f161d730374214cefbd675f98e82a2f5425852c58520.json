<!-- Mission Section -->\n          <div class=\"card shadow-sm mb-5 border-0\">\n            <div class=\"card-body p-5\">\n              <div class=\"row align-items-center\">\n                <div class=\"col-md-6 mb-4 mb-md-0\">\n                  <h2 class=\"fw-bold mb-4\" style=\"color: #2c463f;\">Our Mission</h2>\n                  <p class=\"lead\">We believe financial management should be accessible to everyone. TrackSpend helps you take control of your money without complex spreadsheets or expensive software.</p>\n                </div>\n                <div class=\"col-md-6\">\n                  <div class=\"d-flex justify-content-center\">\n                    <div class=\"feature-icon-lg rounded-circle\" style=\"background-color: rgba(107, 128, 104, 0.1); color: #6b8068;\">\n                      <i class=\"bi bi-bullseye\"></i>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <!-- Features Section -->\n          <div class=\"card shadow-sm mb-5 border-0\">\n            <div class=\"card-body p-5\">\n              <h2 class=\"fw-bold mb-4 text-center\" style=\"color: #2c463f;\">Key Features</h2>\n              <div class=\"row g-4\">\n                <div class=\"col-md-6\" v-for=\"(feature, index) in features\" :key=\"index\">\n                  <div class=\"d-flex\">\n                    <div class=\"feature-icon-sm rounded-circle me-4\" style=\"background-color: #6b8068; color: white;\">\n                      <i :class=\"feature.icon\"></i>\n                    </div>\n                    <div>\n                      <h4 class=\"mb-2\" style=\"color: #2c463f;\">{{ feature.title }}</h4>\n                      <p class=\"mb-0\">{{ feature.description }}</p>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <!-- Team Section -->\n          <div class=\"card shadow-sm border-0\">\n            <div class=\"card-body p-5\">\n              <h2 class=\"fw-bold mb-5 text-center\" style=\"color: #2c463f;\">Meet The Team</h2>\n              <div class=\"row g-4\">\n                <div class=\"col-md-4\" v-for=\"(member, index) in team\" :key=\"index\">\n                  <div class=\"team-member text-center p-4\">\n                    <img \n                      :src=\"member.image\" \n                      :alt=\"member.name\"\n                      class=\"team-avatar mb-3\"\n                      style=\"border-color: #6b8068;\"\n                    >\n                    <h3 class=\"h5 mb-2\" style=\"color: #2c463f;\">{{ member.name }}</h3>\n                    <p class=\"text-muted mb-0\">{{ member.position }}</p>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'AboutPage',\n  data() {\n    return {\n      features: [\n        {\n          icon: 'bi bi-graph-up',\n          title: 'Expense Tracking',\n          description: 'Monitor your spending as it happens with our intuitive dashboard.'\n        },\n        {\n          icon: 'bi bi-wallet2',\n          title: 'Budget Management',\n          description: 'Create personalized categories that match your spending habits.'\n        },\n        {\n          icon: 'bi bi-pie-chart',\n          title: 'Spending Analytics',\n          description: 'Visual reports help you understand your financial patterns.'\n        },\n        {\n          icon: 'bi bi-shield-lock',\n          title: 'Secure Cloud Sync',\n          description: 'Your data is protected and accessible across all devices.'\n        }\n      ],\n      team: [\n        {\n          name: 'Leo Torres',\n          position: 'Founder & Lead Developer',\n          image: require('@/assets/images/leo.jpg')\n        },\n        {\n          name: 'Patrice Virata',\n          position: 'Application Architect',\n          image: require('@/assets/images/patrice.jpg')\n        },\n        {\n          name: 'Félix Scherer',\n          position: 'UX Designer',\n          image: require('@/assets/images/felix.jpg')\n        }\n      ]\n    }\n  }\n}\n</script>\n\n<style scoped>\n.feature-icon-lg {\n  width: 150px;\n  height: 150px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 4rem;\n}\n\n.feature-icon-sm {\n  width: 60px;\n  height: 60px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 1.5rem;\n  flex-shrink: 0;\n}\n\n.team-avatar {\n  width: 150px;\n  height: 150px;\n  border-radius: 50%;\n  object-fit: cover;\n  border: 3px solid;\n  box-shadow: 0 4px 6px rgba(0,0,0,0.1);\n  transition: transform 0.3s ease;\n}\n\n.team-avatar:hover {\n  transform: scale(1.05);\n}\n\n.team-member {\n  transition: all 0.3s ease;\n}\n\n.team-member:hover {\n  transform: translateY(-5px);\n}\n\n.card {\n  border-radius: 12px;\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n}\n\n.card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 10px 20px rgba(0,0,0,0.1) !important;\n}\n\n@media (max-width: 768px) {\n  .feature-icon-lg {\n    width: 100px;\n    height: 100px;\n    font-size: 3rem;\n  }\n  \n  .card-body {\n    padding: 2rem !important;\n  }\n}\n</style>"],"mappings":"AAgFA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QACEC,IAAI,EAAE,gBAAgB;QACtBC,KAAK,EAAE,kBAAkB;QACzBC,WAAW,EAAE;MACf,CAAC,EACD;QACEF,IAAI,EAAE,eAAe;QACrBC,KAAK,EAAE,mBAAmB;QAC1BC,WAAW,EAAE;MACf,CAAC,EACD;QACEF,IAAI,EAAE,iBAAiB;QACvBC,KAAK,EAAE,oBAAoB;QAC3BC,WAAW,EAAE;MACf,CAAC,EACD;QACEF,IAAI,EAAE,mBAAmB;QACzBC,KAAK,EAAE,mBAAmB;QAC1BC,WAAW,EAAE;MACf,EACD;MACDC,IAAI,EAAE,CACJ;QACEN,IAAI,EAAE,YAAY;QAClBO,QAAQ,EAAE,0BAA0B;QACpCC,KAAK,EAAEC,OAAO,CAAC,yBAAyB;MAC1C,CAAC,EACD;QACET,IAAI,EAAE,gBAAgB;QACtBO,QAAQ,EAAE,uBAAuB;QACjCC,KAAK,EAAEC,OAAO