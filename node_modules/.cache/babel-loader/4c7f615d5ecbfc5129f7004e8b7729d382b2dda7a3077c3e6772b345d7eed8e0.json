ction h2 {\n  font-size: 2rem;\n  margin-bottom: 1rem;\n}\n\n.cta-text {\n  margin-bottom: 2rem;\n  opacity: 0.9;\n}\n\n/* Buttons */\n.cta-button {\n  display: inline-block;\n  background: #42b983;\n  color: white;\n  padding: 1rem 2rem;\n  border-radius: 30px;\n  text-decoration: none;\n  font-weight: 600;\n  transition: all 0.3s ease;\n}\n\n.cta-button:hover {\n  background: #359768;\n  transform: translateY(-2px);\n  box-shadow: 0 5px 15px rgba(0,0,0,0.1);\n}\n\n.cta-button-outline {\n  display: inline-block;\n  border: 2px solid #42b983;\n  color: #42b983;\n  padding: 1rem 2rem;\n  border-radius: 30px;\n  text-decoration: none;\n  font-weight: 600;\n  margin-left: 1rem;\n  transition: all 0.3s ease;\n}\n\n.cta-button-outline:hover {\n  background: #42b983;\n  color: white;\n}\n\n@media (max-width: 768px) {\n  .hero-title {\n    font-size: 2rem;\n  }\n\n  .hero-lead {\n    font-size: 1.1rem;\n  }\n\n  .cta-buttons {\n    display: flex;\n    flex-direction: column;\n    gap: 1rem;\n  }\n\n  .cta-button-outline {\n    margin-left: 0;\n  }\n}\n</style>"],"mappings":"AA6DA,eAAe;EACbA,IAAI,EAAE;AACR","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}